{
  "compilerOptions": {
    "target": "es6",
    "lib": ["esnext", "dom"],
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "skipLibCheck": true
  },
  "include": ["src", ".spectral/*"]
}
